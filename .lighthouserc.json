{
    "ci":  {
               "collect":  {
                               "url":  [
                                           "http://localhost:3000/"
                                       ],
                               "numberOfRuns":  1,
                               "settings":  {
                                                "emulatedFormFactor":  "mobile",
                                                "throttlingMethod":  "simulate",
                                                "budgetsPath":  "./budgets.json"
                                            }
                           },
               "assert":  {
                              "assertions":  {
                                                 "categories:performance":  [
                                                                                "error",
                                                                                {
                                                                                    "minScore":  0.90
                                                                                }
                                                                            ],
                                                 "categories:seo":  [
                                                                        "error",
                                                                        {
                                                                            "minScore":  0.90
                                                                        }
                                                                    ],
                                                 "categories:best-practices":  [
                                                                                   "error",
                                                                                   {
                                                                                       "minScore":  0.90
                                                                                   }
                                                                               ]
                                             }
                          },
               "upload":  {
                              "target":  "temporary-public-storage"
                          }
           },
    "budgetsFile":  "./budgets.json"
}
